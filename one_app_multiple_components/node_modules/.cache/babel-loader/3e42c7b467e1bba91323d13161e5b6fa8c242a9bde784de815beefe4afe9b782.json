atteesLists();\n    },\n    updateChattersChatteesLists() {\n      this.chattees = [];\n      for (let i = 0; i < this.users.length; i++) {\n        if (this.users[i].name != this.chatterName) {\n          this.chattees.push(this.users[i]);\n        }\n      }\n    },\n    startChat() {\n      this.chatter.status = 'Online';\n      axios({\n        method: 'put',\n        url: this.usersUrl + '/' + this.chatter.id,\n        data: this.chatter\n      }).then(response => {\n        this.$toast.success('You are online', {\n          position: \"bottom\"\n        });\n      }).catch(error => {\n        this.$toast.error('Error while logging you in', {\n          position: \"bottom\"\n        });\n      });\n      this.ready = true;\n    },\n    logOut() {\n      this.chatter.status = 'Offline';\n      axios({\n        method: 'put',\n        url: this.usersUrl + '/' + this.chatter.id,\n        data: this.chatter\n      }).then(response => {\n        this.$toast.success('You are Offline', {\n          position: \"bottom\"\n        });\n      }).catch(error => {\n        this.$toast.error('Error while logging you in', {\n          position: \"bottom\"\n        });\n      });\n      this.reset();\n    },\n    reset() {\n      this.users = [];\n      this.chatterName = '';\n      this.chatter = {};\n      this.chatteeName = '';\n      this.chattee = {};\n      this.chatters = [];\n      this.chattees = [];\n      this.ready = false;\n      this.fetchUsers();\n      // this.fetchMessages();\n      this.createChatters();\n    }\n  }\n};","map":{"version":3,"mappings":";AA+CA,OAAOA,KAAI,MAAO,OAAO;AACzB,OAAOC,QAAO,MAAO,mCAAmC;AACxD,OAAOC,QAAO,MAAO,mCAAmC;AACxD,OAAOC,gBAAe,MAAO,2CAA2C;AAExE,eAAe;EACfC,IAAI,EAAE,iBAAiB;EACvBC,UAAU,EAAE;IAACJ,QAAQ;IAAEC,QAAQ;IAAEC;EAAgB,CAAC;EAClDG,OAAM,GAAK;IACT,IAAI,CAACC,UAAU,EAAE;IACjBC,WAAW,CAAC,IAAI,CAACC,aAAa,EAAE,GAAG,CAAC;EAGtC,CAAC;EACDC,IAAI,GAAE;IACJ,OAAO;MACLC,KAAK,EAAE,EAAE;MACTC,WAAW,EAAE,EAAE;MACfC,OAAO,EAAE,CAAC,CAAC;MACXC,WAAW,EA