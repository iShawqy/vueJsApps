sage\"\n  ></textarea>\n<!--</div>-->\n</template>\n\n<script>\nimport axios from \"axios\";\n\nexport default {\nname: \"messageInputArea\",\nprops: [\"chatter\", \"chattee\"],\ndata(){\n  return {\n    message:'',\n    usersUrl: \"http://localhost:3004/users\",\n    messagesUrl: \"http://localhost:3004/messages\",\n  }\n\n},\n  methods:{\n  sendMessage(){\n    if (this.message != '') {\n      var currentdate = new Date();\n      var datetime = currentdate.getDate() + \"/\"\n                  + (currentdate.getMonth()+1)  + \"/\"\n                  + currentdate.getFullYear() + \" @ \"\n                  + currentdate.getHours() + \":\"\n                  + currentdate.getMinutes()\n\n\n\n      var data = {\n        content: this.message,\n        timestamp: datetime,\n        creator: this.chatter.name,\n        destination: this.chattee.name,\n      }\n      axios({\n          method: 'post',\n          url:this.messagesUrl,\n          data: data\n        })\n        .then(response => {\n          this.message = '';\n          this.$toast.success('sent',  {\n          position: \"bottom\",\n          duration: 250})\n        })\n        .catch(error => {\n\n          this.$toast.error('Error while logging you in', {\n          position: \"bottom\"});\n        })\n    }\n\n  },\n  updateStatus(){\n    this.chatter.status = 'typing...'\n    axios({\n        method: 'put',\n        url:this.usersUrl + '/' + this.chatter.id,\n        data: this.chatter\n      })\n      .then(response => {\n\n        this.$toast.success('You are Offline',  {\n        position: \"bottom\"})\n      })\n      .catch(error => {\n\n        this.$toast.error('Error while logging you in', {\n        position: \"bottom\"});\n      })\n  },\n  }\n}\n</script>\n\n<style scoped>\n.messageInputMainContainer{\n  display: flex;\n  width: 70%;\n  height: 50px;\n  flex-direction: row;\n  justify-content: flex-start;\n  align-items: flex-start;\n  flex-wrap: wrap;\n  background-color: #00301a;\n\n  bo