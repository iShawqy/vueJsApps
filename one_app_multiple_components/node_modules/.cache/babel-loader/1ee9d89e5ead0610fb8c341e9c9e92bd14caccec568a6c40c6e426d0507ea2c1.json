{"ast":null,"code":"export default {\n  name: \"SignupForm\",\n  props: [\"formTitle\"],\n  data() {\n    return {\n      userInfo: {\n        firstName: 'First name',\n        lastName: 'Last name',\n        age: 'Age',\n        job: 'Job'\n      },\n      userInfoPlaceHolders: {\n        firstName: 'First name',\n        lastName: 'Last name',\n        age: 'Age',\n        job: 'Job'\n      },\n      showError: false,\n      error: 'error message',\n      visible: true\n    };\n  },\n  methods: {\n    removePlaceHolder(keyName) {\n      if (this.userInfo[keyName] == this.userInfoPlaceHolders[keyName]) {\n        this.userInfo[keyName] = '';\n      }\n    },\n    checkEmptyInput(keyName) {\n      if (this.userInfo[keyName].length == 0) {\n        this.userInfo[keyName] = this.userInfoPlaceHolders[keyName];\n        this.showError = true;\n        this.hideError();\n      }\n    },\n    hideError() {\n      setTimeout(() => {\n        this.showError = false;\n      }, 2000);\n    }\n  }\n};","map":{"version":3,"mappings":"AAkDA,eAAe;EACbA,IAAI,EAAE,YAAY;EAClBC,KAAK,EAAE,CAAC,WAAW,CAAC;EACpBC,IAAI,GAAG;IACL,OAAO;MACLC,QAAQ,EAAE;QACRC,SAAS,EAAE,YAAY;QACvBC,QAAQ,EAAE,WAAW;QACrBC,GAAG,EAAE,KAAK;QACVC,GAAG,EAAE;MAAK,CAAC;MACbC,oBAAoB,EAAE;QACpBJ,SAAS,EAAE,YAAY;QACvBC,QAAQ,EAAE,WAAW;QACrBC,GAAG,EAAE,KAAK;QACVC,GAAG,EAAE;MAEL,CAAC;MAEHE,SAAS,EAAE,KAAK;MAChBC,KAAK,EAAE,eAAe;MACtBC,OAAO,EAAE;IAEX;EACF,CAAC;EAGDC,OAAO,EAAE;IAEPC,iBAAiB,CAACC,OAAO,EAAC;MACpB,IAAI,IAAI,CAACX,QAAQ,CAACW,OAAO,KAAK,IAAI,CAACN,oBAAoB,CAACM,OAAO,CAAC,EAAC;QAC/D,IAAI,CAACX,QAAQ,CAACW,OAAO,IAAI,EAAE;MAC7B;IAEN,CAAC;IACDC,eAAe,CAACD,OAAO,EAAC;MACtB,IAAI,IAAI,CAACX,QAAQ,CAACW,OAAO,CAAC,CAACE,MAAK,IAAK,CAAC,EAAC;QACrC,IAAI,CAACb,QAAQ,CAACW,OAAO,IAAI,IAAI,CAACN,oBAAoB,CAACM,OAAO,CAAC;QAC3D,IAAI,CAACL,SAAQ,GAAI,IAAI;QACrB,IAAI,CAACQ,SAAS,EAAC;MACjB;IACF,CAAC;IAEDA,SAAS,GAAE;MACHC,UAAU,CAAC,MAAM;QACT,IAAI,CAACT,SAAQ,GAAI,KAAI;MACzB,CAAC,EAAE,IAAI,CAAC;IAChB;EAEJ;AAGJ","names":["name","props","data","userInfo","firstName","lastName","age","job","userInfoPlaceHolders","showError","error","visible","methods","removePlaceHolder","keyName","checkEmptyInput","length","hideError","setTimeout"],"sourceRoot":"","sources":["/Users/ihm/myDrive/private/learning_vue/user_signup/src/components/forms/SignupForm.vue"],"sourcesContent":["<template>\n\n  <link\n    rel=\"stylesheet\"\n    href=\"https://cdnjs.cloudflare.com/ajax/libs/animate.css/4.1.1/animate.min.css\"\n  />\n<form class=\"customForm\" v-if=\"visible\">\n  <div class=\"topBar\">\n    <button class=\"topBarButtons\">\n    x\n\n    </button>\n\n\n  </div>\n  <div class=\"baseStyle formTitle\">\n    {{formTitle}}\n  </div>\n\n\n  <div>\n    <input id=\"firstname\" @focusout=\"checkEmptyInput('firstName')\" @click=\"removePlaceHolder('firstName')\"\n           v-model.trim=\"userInfo.firstName\" class=\"baseStyle input\" type=\"text\" name=\"text\"/>\n  </div>\n  <div>\n    <input id=\"lastname\" @focusout=\"checkEmptyInput('lastName')\" @click=\"removePlaceHolder('lastName')\"\n           v-model.trim=\"userInfo.lastName\" class=\"baseStyle input\" type=\"text\" name=\"text\" />\n  </div>\n\n  <div>\n    <input id=\"age\" @focusout=\"checkEmptyInput('age')\" @click=\"removePlaceHolder('age')\"\n           v-model.trim=\"userInfo.age\" class=\"baseStyle input\" type=\"text\" name=\"text\"/>\n  </div>\n\n  <div>\n    <input id=\"job\"  @focusout=\"checkEmptyInput('job')\" @click=\"removePlaceHolder('job')\"\n           v-model.trim=\"userInfo.job\" class=\"baseStyle input\" type=\"text\" name=\"text\"/>\n  </div>\n\n  <div class=\"baseStyle error_label animate__animated animate__fadeInDown \"\n        v-if=\"showError\">\n              {{error}}\n  </div>\n  <div class=\"submitBtnContainer\">\n    <input class=\"submitBtn\" type=\"submit\" value=\"Send\" />\n  </div>\n</form>\n</template>\n\n<script>\nexport default {\n  name: \"SignupForm\",\n  props: [\"formTitle\"],\n  data() {\n    return {\n      userInfo: {\n        firstName: 'First name',\n        lastName: 'Last name',\n        age: 'Age',\n        job: 'Job'},\n      userInfoPlaceHolders: {\n        firstName: 'First name',\n        lastName: 'Last name',\n        age: 'Age',\n        job: 'Job'\n\n        },\n\n      showError: false,\n      error: 'error message',\n      visible: true,\n\n    }\n  },\n\n\n  methods: {\n\n    removePlaceHolder(keyName){\n          if (this.userInfo[keyName] == this.userInfoPlaceHolders[keyName]){\n            this.userInfo[keyName] = '';\n          }\n\n    },\n    checkEmptyInput(keyName){\n      if (this.userInfo[keyName].length == 0){\n        this.userInfo[keyName] = this.userInfoPlaceHolders[keyName];\n        this.showError = true;\n        this.hideError()\n      }\n    },\n\n    hideError(){\n            setTimeout(() => {\n                    this.showError = false\n                }, 2000);\n        }\n\n    },\n\n\n}\n\n\n\n\n\n\n</script>\n\n<style scoped>\n      .baseStyle{\n        font-size: 20px;\n        font-family: \"Arial Unicode MS\";\n        /*color: #ffffff;*/\n      }\n      .topBar{\n        display: flex;\n      flex-direction: row;\n      align-items: flex-end;\n      justify-content: flex-end;\n      width: 100%;\n      }\n\n      .topBarButtons{\n        cursor: pointer;\n        border-radius: 50%;\n        background-color: red;\n        color: #0e0404;\n        /*margin-top: 5px;*/\n        /*margin-right: 5px;*/\n        border: none;\n        width: 15px;\n        height: 15px;\n        /*font-size: 10px;*/\n        /*!*align-items: center;*!*/\n        /*text-align: center;*/\n\n        /*cross(40px, #000, 4px);*/\n        position: absolute;\n  top: 50%;\n  /*right: 0%;*/\n  /*padding: 12px 16px;*/\n  transform: translate(0%, -50%);\n      }\n\n      .error_label {\n          color: #f44336;\n          font-size: 12px;\n          text-align: center;\n          margin-top: 10px;\n        }\n\n\n\n    .formTitle{\n      font-size: 16px;\n      color: #9b9b9b;\n      margin-top: 20px;\n      margin-left: 20px;\n      margin-bottom: 30px;\n    }\n\n    .input{\n      width: 110%;\n      border-radius: 10px;\n      height: 40px;\n      border: #662323;\n      color: #0e0404;\n      margin-bottom: 5px;\n      margin-left: 20px;\n      background-color: transparent;\n\n    }\n\n    .customForm{\n      display: flex;\n      flex-direction: column;\n      align-items: flex-start;\n      justify-content: flex-start;\n      width: fit-content;\n      min-width: 300px;\n      height: fit-content;\n      border-radius: 20px;\n\n      /*background: linear-gradient(#086fe9, #3d416e);*/\n      background-color: white;\n      box-shadow: 5px 5px 3px  #000000;\n    }\n\n    .customForm:hover {\n        transform: scale(1.01); /* (150% zoom - Note: if the zoom is too large, it will go outside of the viewport) */\n      }\n\n    /*.submitBtn{*/\n    /*  background-color: #2b5bd2;*/\n    /*  border-radius: 5px;*/\n    /*  color: white;*/\n    /*  font-size: 14px;*/\n    /*  width: 5vw;*/\n    /*  min-width: 5vw;*/\n    /*}*/\n\n      .submitBtnContainer{\n        display: flex;\n      flex-direction: row;\n      align-items: center;\n      justify-content: center;\n      width: 100%;\n      }\n    .submitBtn {\n      border-radius: 10px;\n      background: #0071e3;\n      color: #ffffff;\n      padding: 10px;\n      box-sizing: border-box;\n      border: none;\n      cursor: pointer;\n      /*width: 5vw;*/\n      min-width: 100px;\n      font-size: 21px;\n      margin-top: 5px;\n      margin-bottom: 20px;\n\n    }\n\n    .submitBtn:hover {\n      background: #78c1e0;\n    }\n\n\n</style>"]},"metadata":{},"sourceType":"module","externalDependencies":[]}