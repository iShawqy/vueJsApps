\"result\">\n\n    </resultContainer>\n    <buttons-container @btnClicked=\"executeClick\">\n\n    </buttons-container>\n  </div>\n\n\n</template>\n\n<script>\nimport SignupForm from \"@/components/forms/SignupForm\";\nimport appleCalculaterButton from \"@/components/customButtons/appleCalculaterButton\";\nimport ButtonsContainer from \"@/components/calculatorContainers/ButtonsContainer\";\nimport resultContainer from \"@/components/calculatorContainers/resultContainer\";\nimport ResultContainer from \"@/components/calculatorContainers/resultContainer\";\n\nexport default {\n\n  name: 'App',\n  components: {ResultContainer, ButtonsContainer, SignupForm, appleCalculaterButton},\n  mounted() {\n    setInterval(() => {\n              this.checkResultLength();\n            }, 50);\n  },\n  data (){\n    return {\n      result: '0',\n      actionBtns: ['C', '+/-', '%', 'รท',\n                    'X', '-', '+', '=']\n    }\n  },\n\n  methods: {\n    checkResultLength(){\n      if (this.result.length >11){\n        this.result = this.result.slice(0, 11);\n      }\n    },\n    executeClick(value){\n      if (this.actionBtns.includes(value)){\n        console.log(value);\n        if (value == 'C') {\n          this.result = '0';\n        }\n        else if (value == '+/-') {\n          let oldResult = this.result;\n          if (!oldResult.includes('-')){\n            this.result = '-';\n            this.result += oldResult;\n          }\n\n        }\n        else if (value == '%') {\n          let tempResult = parseFloat(this.result);\n          tempResult = tempResult/100;\n          this.result = tempResult.toString();\n        }\n      }\n      else {\n\n        if (this.result == '0'){\n          let oldResult = this.result;\n          this.result = oldResult;\n          this.result += value;\n        } else {\n          this.result += value;\n\n\n        }\n\n      }\n\n    }\n  },\n\n\n}\n</script>\n\n<style>\n  body {\n  font-family: \"Lucinda Grande\";\n  padding: 0;\n  margin: 0;\n    background-color: #000000;\n    /*apple offwhite*/\n    /*background-color: #f5f5f7;*/\n}\n\n\n\n    .mainContainer {\n      display: flex;\n      flex-direction: column;\n      align-items: center;\n      justify-content: flex-start;\n      position: absolute;\n      left: 50%;\n      top: 50%;\n      -webkit-transform: translate(-50%, -50%);\n      transform: translate(-50%, -50%);\n      /*border-radius: 20px;*/\n\n  }\n\n\n\n</style>\n\n"]},"metadata":{},"sourceType":"module","externalDependencies":[]}