{"ast":null,"code":"import SignupForm from \"@/components/forms/SignupForm\";\nimport appleCalculaterButton from \"@/components/customButtons/appleCalculaterButton\";\nimport ButtonsContainer from \"@/components/calculatorContainers/ButtonsContainer\";\nimport resultContainer from \"@/components/calculatorContainers/resultContainer\";\nimport ResultContainer from \"@/components/calculatorContainers/resultContainer\";\nexport default {\n  name: 'App',\n  components: {\n    ResultContainer,\n    ButtonsContainer,\n    SignupForm,\n    appleCalculaterButton\n  },\n  mounted() {\n    setInterval(() => {\n      this.checkResultLength();\n    }, 50);\n  },\n  data() {\n    return {\n      result: '0',\n      actionBtns: ['C', '+/-', '%', 'รท', 'X', '-', '+', '=']\n    };\n  },\n  methods: {\n    checkResultLength() {\n      if (this.result.length > 11) {\n        this.result = this.result.slice(0, 11);\n      }\n    },\n    executeClick(value) {\n      if (this.actionBtns.includes(value)) {\n        console.log(value);\n        if (value == 'C') {\n          this.result = '0';\n        } else if (value == '+/-') {\n          let oldResult = this.result;\n          if (!oldResult.includes('-')) {\n            this.result = '-';\n            this.result += oldResult;\n          }\n        } else if (value == '%') {\n          let tempResult = parseFloat(this.result);\n          tempResult = tempResult / 100;\n          this.result = tempResult.toString();\n        }\n      } else {\n        if (this.result == '0') {\n          let oldResult = this.result;\n          this.result = oldResult;\n          this.result += value;\n        } else {\n          this.result += value;\n        }\n      }\n    }\n  }\n};","map":{"version":3,"mappings":"AAoBA,OAAOA,UAAS,MAAO,+BAA+B;AACtD,OAAOC,qBAAoB,MAAO,kDAAkD;AACpF,OAAOC,gBAAe,MAAO,oDAAoD;AACjF,OAAOC,eAAc,MAAO,mDAAmD;AAC/E,OAAOC,eAAc,MAAO,mDAAmD;AAE/E,eAAe;EAEbC,IAAI,EAAE,KAAK;EACXC,UAAU,EAAE;IAACF,eAAe;IAAEF,gBAAgB;IAAEF,UAAU;IAAEC;E