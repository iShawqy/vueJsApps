{"ast":null,"code":"import axios from \"axios\";\nexport default {\n  name: \"messageInputArea\",\n  props: [\"chatter\", \"chattee\"],\n  mounted() {\n    setInterval(this.updateStatus, 100);\n  },\n  data() {\n    return {\n      message: '',\n      usersUrl: \"http://localhost:3004/users\",\n      messagesUrl: \"http://localhost:3004/messages\",\n      focused: false,\n      lastState: false\n    };\n  },\n  methods: {\n    sendMessage() {\n      if (this.message != '') {\n        var currentdate = new Date();\n        var datetime = currentdate.getDate() + \"/\" + (currentdate.getMonth() + 1) + \"/\" + currentdate.getFullYear() + \" @ \" + currentdate.getHours() + \":\" + currentdate.getMinutes();\n        var data = {\n          content: this.message,\n          timestamp: datetime,\n          creator: this.chatter.name,\n          destination: this.chattee.name\n        };\n        axios({\n          method: 'post',\n          url: this.messagesUrl,\n          data: data\n        }).then(response => {\n          this.message = '';\n          this.$toast.success('sent', {\n            position: \"bottom\",\n            duration: 250\n          });\n        }).catch(error => {\n          this.$toast.error('Error while logging you in', {\n            position: \"bottom\"\n          });\n        });\n      }\n    },\n    onFocus() {\n      this.focused = true;\n    },\n    outFocus() {\n      this.focused = false;\n    },\n    updateStatus() {\n      if (this.lastState != this.focused) {\n        if (this.focused) {\n          this.chatter.status = 'typing...';\n        } else {\n          this.chatter.status = 'Online';\n        }\n        axios({\n          method: 'put',\n          url: this.usersUrl + '/' + this.chatter.id,\n          data: this.chatter\n        }).then(response => {\n          this.lastState = this.focused;\n        }).catch(error => {\n          this.$toast.error('Error while updating status', {\n            position: \"bottom\"\n          });\n        });\n      }\n    }\n  }\n};","map":{"version":3,"mappings":"AAUA,OAAOA,KAAI,MAAO,OAAO;AAEzB,eAAe;EACfC,IAAI,EAAE,kBAAkB;EACxBC,KAAK,EAAE,CAAC,SAAS,EAAE,SAAS,CAAC;EAC7BC,OAAO,GAAG;IACRC,WAAW,CAAC,IAAI,CAACC,YAAY,EAAE,GAAG;EACpC,CAAC;EACCC,IAAI,GAAE;IACN,OAAO;MACLC,OAAO,EAAC,EAAE;MACVC,QAAQ,EAAE,6BAA6B;MACvCC,WAAW,EAAE,gCAAgC;MAC7CC,OAAO,EAAE,KAAK;MACdC,SAAS,EAAE;IACb;EAEF,CAAC;EACCC,OAAO,EAAC;IACRC,WAAW,GAAE;MACX,IAAI,IAAI,CAACN,OAAM,IAAK,EAAE,EAAE;QACtB,IAAIO,WAAU,GAAI,IAAIC,IAAI,EAAE;QAC5B,IAAIC,QAAO,GAAIF,WAAW,CAACG,OAAO,EAAC,GAAI,GAAE,IAC1BH,WAAW,CAACI,QAAQ,EAAE,GAAC,CAAC,IAAK,GAAE,GAChCJ,WAAW,CAACK,WAAW,EAAC,GAAI,KAAI,GAChCL,WAAW,CAACM,QAAQ,EAAC,GAAI,GAAE,GAC3BN,WAAW,CAACO,UAAU,EAAC;QAIrC,IAAIf,IAAG,GAAI;UACTgB,OAAO,EAAE,IAAI,CAACf,OAAO;UACrBgB,SAAS,EAAEP,QAAQ;UACnBQ,OAAO,EAAE,IAAI,CAACC,OAAO,CAACxB,IAAI;UAC1ByB,WAAW,EAAE,IAAI,CAACC,OAAO,CAAC1B;QAC5B;QACAD,KAAK,CAAC;UACF4B,MAAM,EAAE,MAAM;UACdC,GAAG,EAAC,IAAI,CAACpB,WAAW;UACpBH,IAAI,EAAEA;QACR,CAAC,EACAwB,IAAI,CAACC,QAAO,IAAK;UAChB,IAAI,CAACxB,OAAM,GAAI,EAAE;UACjB,IAAI,CAACyB,MAAM,CAACC,OAAO,CAAC,MAAM,EAAG;YAC7BC,QAAQ,EAAE,QAAQ;YAClBC,QAAQ,EAAE;UAAG,CAAC;QAChB,CAAC,EACAC,KAAK,CAACC,KAAI,IAAK;UAEd,IAAI,CAACL,MAAM,CAACK,KAAK,CAAC,4BAA4B,EAAE;YAChDH,QAAQ,EAAE;UAAQ,CAAC,CAAC;QACtB,CAAC;MACL;IAEF,CAAC;IACDI,OAAO,GAAE;MACP,IAAI,CAAC5B,OAAM,GAAI,IAAI;IACrB,CAAC;IACD6B,QAAQ,GAAE;MACR,IAAI,CAAC7B,OAAM,GAAI,KAAK;IACtB,CAAC;IACDL,YAAY,GAAE;MACZ,IAAI,IAAI,CAACM,SAAQ,IAAK,IAAI,CAACD,OAAO,EAAE;QAClC,IAAI,IAAI,CAACA,OAAO,EAAE;UAChB,IAAI,CAACe,OAAO,CAACe,MAAK,GAAI,WAAU;QAClC,OAAO;UACL,IAAI,CAACf,OAAO,CAACe,MAAK,GAAI,QAAO;QAC/B;QACAxC,KAAK,CAAC;UACJ4B,MAAM,EAAE,KAAK;UACbC,GAAG,EAAC,IAAI,CAACrB,QAAO,GAAI,GAAE,GAAI,IAAI,CAACiB,OAAO,CAACgB,EAAE;UACzCnC,IAAI,EAAE,IAAI,CAACmB;QACb,CAAC,EACAK,IAAI,CAACC,QAAO,IAAK;UAChB,IAAI,CAACpB,SAAQ,GAAI,IAAI,CAACD,OAAO;QAC/B,CAAC,EACA0B,KAAK,CAACC,KAAI,IAAK;UAEd,IAAI,CAACL,MAAM,CAACK,KAAK,CAAC,6BAA6B,EAAE;YACjDH,QAAQ,EAAE;UAAQ,CAAC,CAAC;QACtB,CAAC;MAEH;IAKF;EACA;AACF","names":["axios",