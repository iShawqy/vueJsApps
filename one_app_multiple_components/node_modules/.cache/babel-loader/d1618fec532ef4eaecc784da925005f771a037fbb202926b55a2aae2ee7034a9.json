{"ast":null,"code":"import axios from \"axios\";\nexport default {\n  name: \"messageInputArea\",\n  props: [\"chatter\"],\n  data() {\n    return {\n      message: '',\n      usersUrl: \"http://localhost:3004/users\",\n      messagesUrl: \"http://localhost:3004/messages\"\n    };\n  },\n  methods: {\n    sendMessage() {\n      var currentdate = new Date();\n      var datetime = currentdate.getDate() + \"/\" + (currentdate.getMonth() + 1) + \"/\" + currentdate.getFullYear() + \" @ \" + currentdate.getHours() + \":\" + currentdate.getMinutes();\n      var data = {\n        content: this.message,\n        timestamp: datetime,\n        creator: this.chatter.name\n      };\n      axios({\n        method: 'post',\n        url: this.messagesUrl,\n        data: data\n      }).then(response => {\n        this.message = '';\n        this.$toast.success('sent', {\n          position: \"bottom\",\n          duration: 500\n        });\n      }).catch(error => {\n        this.$toast.error('Error while logging you in', {\n          position: \"bottom\"\n        });\n      });\n    }\n  }\n};","map":{"version":3,"mappings":"AAUA,OAAOA,KAAI,MAAO,OAAO;AAEzB,eAAe;EACfC,IAAI,EAAE,kBAAkB;EACxBC,KAAK,EAAE,CAAC,SAAS,CAAC;EAClBC,IAAI,GAAE;IACJ,OAAO;MACLC,OAAO,EAAC,EAAE;MACVC,QAAQ,EAAE,6BAA6B;MACvCC,WAAW,EAAE;IACf;EAEF,CAAC;EACCC,OAAO,EAAC;IACRC,WAAW,GAAE;MACX,IAAIC,WAAU,GAAI,IAAIC,IAAI,EAAE;MAC5B,IAAIC,QAAO,GAAIF,WAAW,CAACG,OAAO,EAAC,GAAI,GAAE,IAC1BH,WAAW,CAACI,QAAQ,EAAE,GAAC,CAAC,IAAK,GAAE,GAChCJ,WAAW,CAACK,WAAW,EAAC,GAAI,KAAI,GAChCL,WAAW,CAACM,QAAQ,EAAC,GAAI,GAAE,GAC3BN,WAAW,CAACO,UAAU,EAAC;MAIrC,IAAIb,IAAG,GAAI;QACTc,OAAO,EAAE,IAAI,CAACb,OAAO;QACrBc,SAAS,EAAEP,QAAQ;QACnBQ,OAAO,EAAE,IAAI,CAACC,OAAO,CAACnB;MACxB;MACAD,KAAK,CAAC;QACFqB,MAAM,EAAE,MAAM;QACdC,GAAG,EAAC,IAAI,CAAChB,WAAW;QACpBH,IAAI,EAAEA;MACR,CAAC,EACAoB,IAAI,CAACC,QAAO,IAAK;QAChB,IAAI,CAACpB,OAAM,GAAI,EAAE;QACjB,IAAI,CAACqB,MAAM,CAACC,OAAO,CAAC,MAAM,EAAG;UAC7BC,QAAQ,EAAE,QAAQ;UAClBC,QAAQ,EAAE;QAAG,CAAC;MAChB,CAAC,EAEAC,KAAK,